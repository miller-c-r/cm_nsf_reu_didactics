```{r fig.align="center", echo = FALSE,fig.width = 8}


drop_na(scores_and_vitals)%>%
ggplot(scores_and_vitals, mapping = aes(RSA, Dimensional_Score, color= Aggression_Groups))+
  geom_point()+
  geom_line(shape=2)+
 
   labs(
    xlab="Respiratory Sinus Arrythmia Measures in Children",
    ylab="Dimensional Discontinuity Score",
    title = "Discontinuity Score Versus RSA in Children for Four Aggression Groups")+
    scale_y_continuous(limits = c(-5, 5),
                     breaks = seq(-5, 5, by=0.5))+
  scale_x_continuous(limits = c(50, 140),
                    breaks = seq(50, 140, by=5))+
  
  theme_calc()
   


